{
  "name": "dev-center",
  "version": "12.4.2",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "start:aot": "ng serve --aot",
    "build": "npm run clean:prod && ng build --prod --base-href=./",
    "build:prod": "npm run clean:prod && ng build --prod --base-href=./",
    "build:beta": "npm run clean:beta && ng build --configuration=beta --base-href=./ --output-path=./distBeta",
    "test": "npm run lint && ng test --configuration=test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "clean:prod": "rimraf ./dist/",
    "clean:beta": "rimraf ./distBeta/",
    "server": "node ./server.js",
    "prod": "npm run clean:prod && npm run build:prod && npm run server",
    "format:write": "prettier {src,e2e,cypress}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e,cypress}/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run clean:prod && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "compodoc": "compodoc -p src/tsconfig.app.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "jspanel4": "^4.4.0",
    "moment": "^2.23.0",
    "ngx-moment": "^3.3.0",
    "ngx-toastr": "^9.1.1",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular-extensions/testing-library": "^3.1.0",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@compodoc/compodoc": "^1.1.6",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.12.18",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "^0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "karma-spec-reporter": "~0.0.32",
    "lint-staged": "^8.1.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.15.3",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0",
    "typescript": "^3.1.1",
    "typescript-estree": "^9.0.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
